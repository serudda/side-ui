{
  "name": "side-ui",
  "version": "0.1.18",
  "type": "module",
  "packageManager": "pnpm@7.27.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/serudda/side-ui"
  },
  "keywords": [],
  "author": "Indie Creators HQ",
  "bugs": {
    "url": "https://github.com/serudda/side-ui/issues"
  },
  "homepage": "https://github.com/serudda/side-ui#readme",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && pnpm run build:css",
    "build:css": "tailwindcss -m -i ./src/index.css -o ./dist/styles.css",
    "clean": "git clean -xdf node_modules",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .gitignore"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/side-ui.es.js",
      "require": "./dist/side-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/side-ui.umd.js",
  "module": "./dist/side-ui.es.js",
  "types": "./dist/components/index.d.ts",
  "dependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.11",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.7",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-static-copy": "^0.14.0"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  }
}
